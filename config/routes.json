{
    "routes": [
        {
            "path": "/content/assets",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "assets.overview"
        },
        {
            "path": "/content/assets/%locale%/folder",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "assets.overview.locale"
        },
        {
            "path": "/content/assets/%locale%/folder/%folder%",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "assets.folder.overview"
        },
        {
            "path": "/content/assets/%locale%/folder/%folder%/sort",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "sortAction",
            "id": "assets.folder.sort",
            "methods": "post"
        },
        {
            "path": "/content/assets/%locale%/folder/%folder%/edit",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderAction",
            "id": "assets.folder.edit",
            "methods": ["head", "get", "post"]
        },
        {
            "path": "/content/assets/%locale%/folder/%folder%/delete",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderDeleteAction",
            "id": "assets.folder.delete"
        },
        {
            "path": "/content/assets/%locale%/add/folder",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderAction",
            "id": "assets.folder.add"
        },
        {
            "path": "/content/assets/%locale%/asset/%item%/edit/",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "assetAction",
            "id": "asset.edit",
            "methods": ["head", "get", "post"]
        },
        {
            "path": "/content/assets/%locale%/asset/%item%/delete",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "AssetDeleteAction",
            "id": "asset.delete"
        },
        {
            "path": "/content/assets/%locale%/add/item",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "assetAction",
            "id": "asset.add"
        }
    ]
}
