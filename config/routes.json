{
    "routes": [
        {
            "path": "/content/asset",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "asset.overview"
        },
        {
            "path": "/content/asset/%locale%/folder",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "asset.overview.locale"
        },
        {
            "path": "/content/asset/%locale%/folder/%folder%",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "indexAction",
            "id": "asset.folder.overview"
        },
        {
            "path": "/content/asset/%locale%/folder/%folder%/sort",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "sortAction",
            "id": "asset.folder.sort",
            "methods": "post"
        },
        {
            "path": "/content/asset/%locale%/folder/%folder%/edit",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderAction",
            "id": "asset.folder.edit",
            "methods": ["head", "get", "post"]
        },
        {
            "path": "/content/asset/%locale%/folder/%folder%/delete",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderDeleteAction",
            "id": "asset.folder.delete"
        },
        {
            "path": "/content/asset/%locale%/add/folder",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "folderAction",
            "id": "asset.folder.add"
        },
        {
            "path": "/content/asset/%locale%/item/%item%/edit",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "itemAction",
            "id": "asset.item.edit",
            "methods": ["head", "get", "post"]
        },
        {
            "path": "/content/asset/%locale%/item/%item%/delete",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "itemDeleteAction",
            "id": "asset.item.delete"
        },
        {
            "path": "/content/asset/%locale%/add/item",
            "controller": "ride\\web\\cms\\controller\\backend\\AssetController",
            "action": "itemAction",
            "id": "asset.item.add"
        }
    ]
}
